[Unit]
Description=Proxy for minecraft server
After=minecraft-container-shutdown.service
Requires=minecraft-container-shutdown.service

[Service]
Type=simple
ExecStart=/usr/local/bin/frpc -c /usr/local/share/frp/mc-server-proxy-server-frpc.toml
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target

